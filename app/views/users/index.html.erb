		<% @users.each_slice(4) do |three| %>
		  <div class="row">
		    <% three.each do |user| %>
		         <div class="col-md-3">
		          <div class="card-back">
		            <div class="card">
		             <a href="/users/<%= user.id %>"><h3><%= user.first_name %></h3></a><br>
		             <div class="pic">
		             	<%=image_tag("#{user.image}")%>
		             </div>
		             <br>
		             <%= user.bio %><br>
		             <% if user_signed_in? %>
		             	<% if current_user.id == user.id %>
		             		<%= link_to "Edit Profile", edit_user_registration_path %>
		             	<% end %>
		             <% end %>
		            </div>
		          </div>
		        </div>
		    <%end%>
		  </div>
		<% end %>