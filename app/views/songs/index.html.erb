<div id="songs">
			<div class="center-forms">
						<div class="back">
						<h2>Search Songs</h2>
							<input list="songslist" type="text" placeholder="Enter song or artist" class="form-control" v-model="userQuery"><br>
							<button class="btn btn-primary" v-on:click="searchSong()">Search for track</button>
						</div>
						<datalist id="songslist">
				    		<option v-for="song in songs" v-bind:value="song.name + ' - ' + song.artists[0].name">
				  		</datalist>
						<br>
				</div>
						<ul>
							<div class="row">
					 			<div v-for="song in songs">
					 				<div class="col-md-4">
					 					<div class="well">
					 						<div class="item">
							 				 <b>Song name</b>: {{song.name}}<br>
							 				 <b>Artist</b>: {{song.artists[0].name}}<br>
									 		 <p>
									 		 </p>
									 		</div>
									 	</div>
							 		</div>
							 	</div>	
					 		</div>
					 	</ul>
</div>

